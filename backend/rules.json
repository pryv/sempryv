{
  "@context": {
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "pryv": "http://pryv.com/types#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "snomed-ct": "http://purl.bioontology.org/ontology/SNOMEDCT/",
    "skos:broader": {
      "@type": "@id"
    },
    "skos:narower": {
      "@type": "@id"
    },
    "skos:closeMatch": {
      "@type": "@id"
    },
    "skos:broadMatch": {
      "@type": "@id"
    },
    "skos:exactMatch": {
      "@type": "@id"
    },
    "pryv:mapping": {
      "@type": "@id"
    }

  },
  "@graph": [{
      "@id": "pryv:temperature",
      "@type": "skos:Concept",
      "skos:notation": "note/txt",
      "skos:closeMatch": "snomed-ct:386725007",
      "skos:broadMatch": "snomed-ct:386455007"
    },
    {
      "@id": "pryv:bodyTemperature",
      "@type": "skos:Concept",
      "skos:notation": "note/txt",
      "skos:broader": "pryv:temperature",
      "skos:closeMatch": "snomed-ct:386725007"
    },
    {
      "@id": "pryv:heart",
      "@type": "skos:Concept",
      "skos:notation": ["heart","note/txt"],
      "skos:closeMatch": "snomed-ct:36407500"
    },
    {
      "@id": "pryv:mass",
      "@type": "skos:Concept",
      "skos:notation": "mass",
      "skos:closeMatch": "snomed-ct:118538004"
    },
    {
      "@id": "someRuleSet",
      "pryv:pathExpression": ".*/group",
      "pryv:mapping": ["pryv:mass", "pryv:temperature"]

    },
    {
      "@id": "someRuleSet1",
      "pryv:pathExpression": "body temperature/",
      "pryv:mapping": ["pryv:temperature"]

    },
    {
      "@id": "someRuleSet2",
      "pryv:pathExpression": "heart/",
      "pryv:mapping": ["pryv:heart"]

    },
    {
      "@id": "pryv:frequencyHerz",
      "@type": "skos:Concept",
      "skos:notation": "frequency/hz",
      "skos:closeMatch": "loinc:34534"
    },
    {
      "@id": "pryv:notetxt",
      "@type": "skos:Concept",
      "skos:notation": "NOTE/TXT",
      "skos:closeMatch": "snomed-ct:102730007"
    },
    {
      "@id": "someSecondRuleSet",
      "pryv:pathExpression": "/diary.*",
      "pryv:mapping": ["pryv:frequencyHerz", "pryv:notetxt"]
    }
  ]
}